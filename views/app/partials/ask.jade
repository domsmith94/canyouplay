div.container
	div.col-md-12.text-center
		ol.breadcrumb
			li
				a(href='#fixtures') All Fixtures
			li
				a(ng href='#fixture/{{availData.fixtureId}}') Fixture
			li.active Ask

		h2 Ask Players To Play
		h3
			| {{ availData.side }} V {{ availData.opposition }}, {{ availData.date | date:"dd MMMM yyyy"}}
	div.col-md-4.col-md-offset-2
		h4 Players Who Are Availiable On This Date
		ul(ng-repeat="player in availData.playersAvail")
			li {{ player.firstName}} {{ player.lastName}}
	div.col-md-4
		h4 Players Who Are Not Availiable On This Date
		ul(ng-repeat="player in availData.playersNotAvail")
			li {{ player.firstName}} {{ player.lastName}}







